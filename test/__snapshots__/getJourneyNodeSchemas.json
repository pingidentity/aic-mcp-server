{
  "name": "getJourneyNodeSchemas",
  "title": "Get Journey Node Schemas (Batch)",
  "description": "Retrieve schemas for multiple journey node types in a single operation. Returns the schema definition for each requested node type, useful for understanding available node configurations.",
  "scopes": [
    "fr:am:*"
  ],
  "inputSchema": {
    "realm": {
      "_def": {
        "values": [
          "alpha",
          "bravo"
        ],
        "typeName": "ZodEnum",
        "description": "The realm containing the journey nodes"
      },
      "~standard": {
        "version": 1,
        "vendor": "zod"
      }
    },
    "nodeTypes": {
      "_def": {
        "type": {
          "_def": {
            "schema": {
              "_def": {
                "schema": {
                  "_def": {
                    "checks": [
                      {
                        "kind": "min",
                        "value": 1,
                        "message": "Value cannot be empty"
                      }
                    ],
                    "typeName": "ZodString",
                    "coerce": false
                  },
                  "~standard": {
                    "version": 1,
                    "vendor": "zod"
                  }
                },
                "typeName": "ZodEffects",
                "effect": {
                  "type": "refinement"
                }
              },
              "~standard": {
                "version": 1,
                "vendor": "zod"
              }
            },
            "typeName": "ZodEffects",
            "effect": {
              "type": "refinement"
            }
          },
          "~standard": {
            "version": 1,
            "vendor": "zod"
          }
        },
        "minLength": {
          "value": 1
        },
        "maxLength": null,
        "exactLength": null,
        "typeName": "ZodArray",
        "description": "Array of node type names to retrieve schemas for (e.g., ['IncrementLoginCountNode', 'UsernameCollectorNode', 'PasswordCollectorNode'])"
      },
      "~standard": {
        "version": 1,
        "vendor": "zod"
      }
    }
  }
}