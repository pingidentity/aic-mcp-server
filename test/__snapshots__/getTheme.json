{
  "name": "getTheme",
  "title": "Get Theme",
  "description": "Retrieve a specific theme by ID or name from PingOne AIC",
  "scopes": [
    "fr:idm:*"
  ],
  "inputSchema": {
    "realm": {
      "_def": {
        "values": [
          "alpha",
          "bravo"
        ],
        "typeName": "ZodEnum",
        "description": "Realm name"
      },
      "~standard": {
        "version": 1,
        "vendor": "zod"
      }
    },
    "themeIdentifier": {
      "_def": {
        "checks": [],
        "typeName": "ZodString",
        "coerce": false,
        "description": "Theme ID or name"
      },
      "~standard": {
        "version": 1,
        "vendor": "zod"
      }
    }
  }
}